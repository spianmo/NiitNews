<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- solid表示远的填充色 -->
    <solid android:color="@color/picture_color_ffe85d" />
    <!-- stroke则代表远的边框线 -->
    <stroke
        android:width="1dp"
        android:color="@color/picture_color_ffe85d" />
    <!-- size控制高宽 -->
    <size
        android:width="12dp"
        android:height="12dp" />
</shape>